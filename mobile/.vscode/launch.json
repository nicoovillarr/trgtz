  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_${input:flavor}.dart",
      "args": ["--flavor", "${input:flavor}", "--target", "lib/main_${input:flavor}.dart"]
    }
  ],
  "inputs": [
    {
      "id": "flavor",
      "type": "pickString",
      "description": "Select a flavor",
      "options": ["development", "staging", "live"]
    }
  ]
}
